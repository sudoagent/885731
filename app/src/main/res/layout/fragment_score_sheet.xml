<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:theme="@style/Theme.MyApp.NoActionBar"
    android:paddingLeft="8dp"
    android:paddingRight="8dp">
    <LinearLayout
        android:id="@+id/FirstBattingTeamLine"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/FirstBattingTeam"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:textColor="@color/secondary_text_color"
            android:paddingTop="16dp"
            android:paddingBottom="16dp"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:textSize="18dp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/FirstBattingTeamScore"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:textColor="@color/secondary_text_color"
            android:paddingTop="16dp"
            android:paddingBottom="16dp"
            android:textSize="16dp"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:textStyle="bold" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/SecondBattingTeamLine"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/SecondBattingTeam"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:textColor="@color/secondary_text_color"
            android:paddingTop="16dp"
            android:paddingBottom="16dp"
            android:textSize="18dp"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/SecondBattingTeamScore"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/secondary_text_color"
            android:gravity="end"
            android:paddingTop="16dp"
            android:textSize="16dp"
            android:paddingBottom="16dp"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:textStyle="bold" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/secondary_text_color"
            android:textStyle="bold"
            android:layout_weight=".10"
            android:text="CRR :" />
        <TextView
            android:id="@+id/currRunRate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/secondary_text_color"
            android:layout_weight="0.20" />
        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/secondary_text_color"
            android:textStyle="bold"
            android:layout_weight="0.10"
            android:text="RRR :" />
        <TextView
            android:id="@+id/reqRunrate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/secondary_text_color"
            android:layout_weight="0.20" />
        <TextView
            android:id="@+id/toWin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/secondary_text_color"
            android:layout_weight="0.40"
            android:textDirection="rtl"
            android:text="N/A" />
    </LinearLayout>
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="4dp"
        android:paddingRight="4dp">
        <TableRow
            android:id="@+id/batsmen_header"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="4dp"
            android:dividerPadding="8dp">
            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".35"
                android:textColor="@color/secondary_text_color"
                android:textStyle="bold"
                android:text="Batsmen" />
            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".15"
                android:textColor="@color/secondary_text_color"
                android:textStyle="bold"
                android:text="R" />
            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".15"
                android:textColor="@color/secondary_text_color"
                android:textStyle="bold"
                android:text="B" />
            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".1"
                android:textColor="@color/secondary_text_color"
                android:textStyle="bold"
                android:text="4s" />
            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".1"
                android:textColor="@color/secondary_text_color"
                android:textStyle="bold"
                android:text="6s" />
            <TextView
                android:id="@+id/textView10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight=".2"
                android:textAlignment="textEnd"
                android:textColor="@color/secondary_text_color"
                android:textStyle="bold"
                android:text="S/R" />
        </TableRow>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/hinttext_underline_divider_color" />
        <TableRow
            android:id="@+id/batsmen_row1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:dividerPadding="8dp">
            <TextView
                android:id="@+id/striker_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".35"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/striker_runs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".15"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/striker_balls"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".15"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/striker_fours"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".1"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/striker_sixes"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".1"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/striker_strikerate"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight=".2"
                android:textAlignment="textEnd"
                android:textColor="@color/secondary_text_color" />
        </TableRow>
        <TableRow
            android:id="@+id/batsmen_row2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:dividerPadding="8dp">
            <TextView
                android:id="@+id/non_striker_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".35"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/non_striker_runs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".15"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/non_striker_balls"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".15"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/non_striker_fours"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".1"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/non_striker_sixes"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".1"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/non_striker_strikerate"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight=".2"
                android:textAlignment="textEnd"
                android:textColor="@color/secondary_text_color" />
        </TableRow>
    </TableLayout>
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="4dp"
        android:paddingRight="4dp">
        <TableRow
            android:id="@+id/bowler_header"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="4dp"
            android:dividerPadding="8dp">
            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".35"
                android:textColor="@color/secondary_text_color"
                android:textStyle="bold"
                android:text="Bowlers" />
            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".15"
                android:textColor="@color/secondary_text_color"
                android:textStyle="bold"
                android:text="O" />
            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".1"
                android:textColor="@color/secondary_text_color"
                android:textStyle="bold"
                android:text="M" />
            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".15"
                android:textColor="@color/secondary_text_color"
                android:textStyle="bold"
                android:text="R" />
            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".1"
                android:textColor="@color/secondary_text_color"
                android:textStyle="bold"
                android:text="W" />
            <TextView
                android:id="@+id/textView16"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight=".2"
                android:textAlignment="textEnd"
                android:textColor="@color/secondary_text_color"
                android:textStyle="bold"
                android:text="ECON" />
        </TableRow>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/hinttext_underline_divider_color" />
        <TableRow
            android:id="@+id/bowler_row1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:dividerPadding="8dp">
            <TextView
                android:id="@+id/current_bowler_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".35"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/current_bowler_overs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".15"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/current_bowler_maidens"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".1"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/current_bowler_runs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".15"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/current_bowler_wickets"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".1"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/current_bowler_economy"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight=".2"
                android:textAlignment="textEnd"
                android:textColor="@color/secondary_text_color" />
        </TableRow>
        <TableRow
            android:id="@+id/bowler_row2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:dividerPadding="8dp">
            <TextView
                android:id="@+id/previous_bowler_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".35"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/previous_bowler_overs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".15"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/previous_bowler_maidens"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".1"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/previous_bowler_runs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".15"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/previous_bowler_wickets"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".1"
                android:textColor="@color/secondary_text_color" />
            <TextView
                android:id="@+id/previous_bowler_economy"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight=".2"
                android:textAlignment="textEnd"
                android:textColor="@color/secondary_text_color" />
        </TableRow>
    </TableLayout>
    <LinearLayout
        android:id="@+id/recent_last_wicket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="16dp"
        android:paddingBottom="8dp"
        android:paddingLeft="4dp"
        android:orientation="vertical"
        android:paddingRight="4dp">
        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/hinttext_underline_divider_color" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/textView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/secondary_text_color"
                android:textStyle="bold"
                android:text="RECENT " />
            <TextView
                android:id="@+id/recent_deliveries"
                android:textColor="@color/secondary_text_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/hinttext_underline_divider_color" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:paddingLeft="4dp">
            <TextView
                android:id="@+id/TextView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/secondary_text_color"
                android:paddingRight="8dp"
                android:textSize="10dp"
                android:textStyle="bold"
                android:text="Last Wicket: " />
            <TextView
                android:id="@+id/last_batsmen_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/secondary_text_color"
                android:paddingRight="8dp"
                android:textSize="10dp"
                android:text="Last Batsmen" />
            <TextView
                android:id="@+id/last_batsmen_score"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/secondary_text_color"
                android:layout_weight="1"
                android:paddingLeft="8dp"
                android:textSize="10dp"
                android:text="0 (0b)" />
            <TextView
                android:id="@+id/fow_textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/secondary_text_color"
                android:paddingRight="8dp"
                android:textSize="10dp"
                android:text="FoW" />
            <TextView
                android:id="@+id/fall_of_wicket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/secondary_text_color"
                android:layout_weight="1"
                android:textSize="10dp"
                android:paddingLeft="8dp"
                android:text="0/0" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/buttons_line_one_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="3dp"
            android:paddingLeft="3dp"
            android:paddingRight="3dp">
            <com.google.android.material.button.MaterialButton
                android:id="@+id/dotBall"
                android:layout_width="54dp"
                android:layout_height="54dp"
                android:gravity="center"
                android:elevation="1pt"
                app:cornerRadius="6dp"
                android:text="0"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="3dp"
            android:paddingLeft="3dp"
            android:paddingRight="3dp">
            <com.google.android.material.button.MaterialButton
                android:id="@+id/oneRun"
                android:layout_width="54dp"
                android:layout_height="54dp"
                android:gravity="center"
                android:elevation="1pt"
                app:cornerRadius="6dp"
                android:text="1"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="3dp"
            android:paddingLeft="3dp"
            android:paddingRight="3dp">
            <com.google.android.material.button.MaterialButton
                android:id="@+id/twoRun"
                android:layout_width="54dp"
                android:layout_height="54dp"
                android:gravity="center"
                android:elevation="1pt"
                app:cornerRadius="6dp"
                android:text="2"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="3dp"
            android:paddingLeft="3dp"
            android:paddingRight="3dp">
            <com.google.android.material.button.MaterialButton
                android:id="@+id/threeRun"
                android:layout_width="53dp"
                android:layout_height="54dp"
                android:gravity="center"
                android:elevation="1pt"
                app:cornerRadius="6dp"
                android:text="3"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="3dp"
            android:paddingLeft="3dp"
            android:paddingRight="3dp">
            <com.google.android.material.button.MaterialButton
                android:id="@+id/fiveRuns"
                android:layout_width="54dp"
                android:layout_height="54dp"
                android:gravity="center"
                android:elevation="1pt"
                app:cornerRadius="6dp"
                android:text="5"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="3dp"
            android:paddingLeft="3dp"
            android:paddingRight="3dp">
            <com.google.android.material.button.MaterialButton
                android:id="@+id/undoBall"
                android:layout_width="54dp"
                android:layout_height="54dp"
                android:gravity="center"
                android:elevation="1pt"
                app:cornerRadius="6dp"
                app:icon="@drawable/ic_undo_black_24dp"
                app:iconGravity="textStart"
                android:layout_gravity="center"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/buttons_line_two_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="3dp"
            android:paddingLeft="3dp"
            android:paddingRight="3dp">
            <com.google.android.material.button.MaterialButton
                android:id="@+id/fourRuns"
                android:layout_width="120dp"
                android:layout_height="54dp"
                android:gravity="center"
                android:elevation="1pt"
                app:cornerRadius="6dp"
                android:text="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="3dp"
            android:paddingLeft="3dp"
            android:paddingRight="3dp">
            <com.google.android.material.button.MaterialButton
                android:id="@+id/sixRuns"
                android:layout_width="120dp"
                android:layout_height="54dp"
                android:gravity="center"
                android:elevation="1pt"
                app:cornerRadius="6dp"
                android:text="6"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/custom_spinner_row_one_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="8dp"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:paddingBottom="8dp"
            android:orientation="vertical">
            <Spinner
                android:id="@+id/wicket_spinner"
                android:theme="@style/DropDownListViewStyle"
                android:layout_width="wrap_content"
                android:background="@drawable/spinner_bg"
                android:dropDownHeight="250dp"
                android:paddingTop="6dp"
                android:paddingBottom="6dp"
                android:layout_height="?attr/dropdownListPreferredItemHeight"
                android:layout_marginLeft="8dp"
                android:textColor="@color/primary_text_color" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/custom_spinner_row_two_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:paddingTop="8dp"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:paddingBottom="8dp">
            <Spinner
                android:id="@+id/extras"
                android:theme="@style/DropDownListViewStyle"
                android:layout_width="wrap_content"
                android:background="@drawable/spinner_bg"
                android:dropDownHeight="250dp"
                android:paddingTop="6dp"
                android:paddingBottom="6dp"
                android:layout_height="?attr/dropdownListPreferredItemHeight"
                android:layout_marginLeft="8dp"
                android:textColor="@color/primary_text_color" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/base_layout_score_fragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    </LinearLayout>
</LinearLayout>